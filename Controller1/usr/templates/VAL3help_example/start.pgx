<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2">
  <Program name="start">
    <Code><![CDATA[begin
  // Init variables
  sPrompt="Please enter your input:"
  sInput=""
  sOutput=""
  bExit = false
  
  // Display user page
  nRet=userPage("main")
  
  if (userPagePrompt(sPrompt, sInput) == true)
    // TODO: 
    // Add hereafter your code with sInput as input and sOutput as output
    // For example sOutput=sInput
     sOutput=sInput
  else
    sOutput="User canceled prompt"
  endIf
  
  // Wait for button exit to be pressed
  while bExit==false
    delay(0)
  endWhile
end]]></Code>
  </Program>
</Programs>